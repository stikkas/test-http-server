<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="my.home.pro.test.MainController">
   <center>
      <SplitPane dividerPositions="0.5" prefHeight="402.0" prefWidth="540.0">
