            <ScrollPane prefHeight="400.0" prefWidth="887.0">
               <content>
                  <VBox fx:id="paneBox" prefHeight="200.0" prefWidth="265.0" />
               </content>
            </ScrollPane>
            <TextArea fx:id="resultPane" editable="false" prefHeight="400.0" prefWidth="525.0" />
         </items>
      </SplitPane>
   </center>
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#launchButtonClicked" text="Launch" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
            <Button mnemonicParsing="false" onAction="#addButtonClicked" text="Add" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
            <Button layoutX="391.0" layoutY="5.0" mnemonicParsing="false" onAction="#clearArea" text="Clear" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="5.0" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
